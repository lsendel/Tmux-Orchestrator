{
  "orchestrator": {
    "coding_directory": "~/IdeaProjects",
    "check_interval_minutes": 30,
    "max_agents_per_session": 5,
    "enable_auto_recovery": true,
    "log_level": "INFO"
  },
  "claude": {
    "startup_delay_seconds": 5,
    "message_delay_seconds": 0.5,
    "health_check_timeout": 10
  },
  "git": {
    "auto_commit_interval_minutes": 30,
    "commit_message_prefix": "Auto-commit",
    "enable_auto_push": false
  },
  "notifications": {
    "enable_desktop_notifications": true,
    "slack_webhook_url": "",
    "email_alerts": false
  },
  "resource_limits": {
    "max_sessions": 10,
    "max_agents_total": 20,
    "memory_limit_gb": 8
  }
}